<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
   <!-- xmlns:android="http://schemas.android.com/apk/res/android">-->
    <EditTextPreference
        android:title="@string/pref_location"
        android:key="@string/pref_location_key"
        android:defaultValue="@string/pref_location_default"
        android:inputType="text"
        android:singleLine="true"/>
    <ListPreference
        android:title="@string/pref_metric"
        android:key="@string/pref_metric_key"
        android:entries="@array/pref_metric_options"
        android:entryValues="@array/pref_metric_options_values"
        android:defaultValue="@string/pref_metric_default"/>

</PreferenceScreen>